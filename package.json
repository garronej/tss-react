{
    "name": "tss-react",
    "version": "0.0.12",
    "description": "Like JSS but for TypeScript",
    "repository": {
        "type": "git",
        "url": "git://github.com/garronej/tss-react.git"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "tsc",
        "link": "node dist/bin/link",
