{
    "name": "tss-react",
    "version": "3.3.2",
    "description": "makeStyles is dead, long live makeStyles!",
    "repository": {
        "type": "git",
        "url": "git://github.com/garronej/tss-react.git"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "tsc",
        "start_spa": "yarn yarn_link && cd src/test/apps/spa && yarn start",
        "start_ssr": "yarn yarn_link && cd src/test/apps/ssr && yarn dev",
        "start_muiV4": "yarn yarn_link && cd src/test/apps/muiV4ssr && yarn dev",
        "lint:check": "eslint . --ext .ts,.tsx",
        "lint": "yarn lint:check --fix",
        "_format": "prettier '**/*.{ts,tsx,json,md}'",
        "format": "yarn _format --write",
        "format:check": "yarn _format --list-different",
        "yarn_link": "ts-node src/bin/yarn_link.ts"
    },
    "lint-staged": {
        "*.{ts,tsx}": [
            "eslint --fix"
        ],
        "*.{ts,tsx,json,md}": [
            "prettier --write"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged -v"
        }
    },
    "author": "u/garronej",
    "license": "MIT",
    "files": [
        "dist/",
        "!dist/test/",
        "!dist/bin/",
        "!dist/package.json",
        "!dist/tsconfig.tsbuildinfo"
    ],
    "keywords": [
        "jss",
        "hooks",
        "react",
        "@material-ui"
    ],
    "homepage": "https://github.com/garronej/tss-react",
    "peerDependencies": {
        "@emotion/react": "^11.4.1"
    },
    "dependencies": {
        "@emotion/serialize": "*",
        "@emotion/utils": "*"
    },
    "devDependencies": {
        "@emotion/server": "^11.4.0",
        "@emotion/react": "^11.4.1",
        "@mui/material": "^5.0.3",
        "@emotion/styled": "^11.3.0",
        "@types/node": "^15.3.1",
        "@types/react": "^17.0.2",
        "@types/react-dom": "^17.0.9",
        "@typescript-eslint/eslint-plugin": "^4.24.0",
        "@typescript-eslint/parser": "^4.24.0",
        "eslint": "^7.26.0",
        "eslint-config-prettier": "^8.3.0",
        "husky": "^4.3.8",
        "lint-staged": "^11.0.0",
        "next": "11.0.1",
        "prettier": "^2.3.0",
        "react": "^17.0.2",
        "ts-node": "^10.2.1",
        "tsafe": "^0.7.1",
        "typescript": "^4.4.3"
    }
}
