{
    "name": "tss-react",
    "version": "0.0.12",
    "description": "Like JSS but for TypeScript",
    "repository": {
        "type": "git",
        "url": "git://github.com/garronej/tss-react.git"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "tsc",
        "link_test_projects_dependencies": "node dist/bin/link_test_projects_dependencies",
        "copy_shared": "cd src/test && cp -r ssr/src/shared spa/pages",
        "start_spa": "yarn link_test_projects_dependencies && yarn copy_shared && cd src/test/spa && yarn start",
        "start_ssr": "yarn link_test_projects_dependencies && cd src/test/ssr && yarn dev",
        "prepare": "yarn build"
    },
    "author": "u/garronej",
    "license": "MIT",
    "files": [
        "src/",
        "!src/test/",
        "!src/bin/",
        "dist/",
        "!dist/test/",
        "!dist/bin/",
        "!dist/package.json",
        "!dist/tsconfig.tsbuildinfo"
    ],
    "keywords": [
        "jss",
        "hooks",
        "react",
        "@material-ui"
    ],
    "homepage": "https://github.com/garronej/tss-react",
    "dependencies": {
        "@emotion/cache": "^11.0.0",
        "@emotion/react": "^11.0.0",
        "@emotion/serialize": "^1.0.0",
        "@emotion/server": "^11.4.0",
        "@emotion/utils": "^1.0.0",
        "html-react-parser": "^1.2.7"
    },
    "devDependencies": {
        "@types/node": "^10.0.0",
        "@types/react": "^17.0.2",
        "@types/react-dom": "^17.0.2",
        "evt": "beta",
        "minimal-polyfills": "^2.1.6",
        "next": "^11.0.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "tsafe": "^0.4.1",
        "typescript": "^4.3.5"
    }
}
