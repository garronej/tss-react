    "license": "MIT",
    "files": [
        "src/",
        "!src/test/",
        "!src/bin/",
        "dist/",
        "!dist/test/",
        "!dist/bin/",
        "!dist/tsconfig.tsbuildinfo"
    ],
    "keywords": [],
    "homepage": "https://github.com/garronej/tss-react",
    "dependencies": {
        "@emotion/css": "^11.1.3",
        "@emotion/react": "^11.4.0"
    },
    "devDependencies": {
        "@types/memoizee": "^0.4.5",
        "@types/node": "^10.0.0",
        "@types/react": "^17.0.2",
        "denoify": "^0.6.4",
        "evt": "^1.9.12",
        "minimal-polyfills": "^2.1.6",
        "lint-staged": "^10.5.4",
        "memoizee": "^0.4.15",
        "typescript": "^4.3.5"
    }
